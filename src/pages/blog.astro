---
import BaseLayout from '../layouts/BaseLayout.astro';

const posts = [
  { slug: "tts", title: "Qwen-TTS", image: "/wp-content/uploads/2025/06/Qwen-TTS.webp" },
  { slug: "personal-assistant", title: "Qwen AI Personal Assistant", image: "/wp-content/uploads/2025/06/Qwen-AI-Personal-Assistant.webp" },
  { slug: "for-finance", title: "Qwen AI for Finance", image: "/wp-content/uploads/2025/06/Qwen-AI-for-Finance.webp" },
  { slug: "for-content-creation", title: "Qwen AI for Content Creation", image: "/wp-content/uploads/2025/06/Qwen-AI-for-Content-Creation.webp" },
  { slug: "vs-mistral", title: "Qwen vs Mistral", image: "/wp-content/uploads/2025/06/Qwen-vs-Mistral.webp" },
  { slug: "roadmap", title: "Qwen AI Roadmap 2025â€“2026", image: "/wp-content/uploads/2025/06/Qwen-AI-Roadmap.webp" },
  { slug: "limitations", title: "Qwen AI Limitations 2025", image: "/wp-content/uploads/2025/06/Qwen-AI-Limitations.webp" },
  { slug: "safety", title: "Qwen AI Safety 2025", image: "/wp-content/uploads/2025/06/Qwen-AI-Safety.webp" },
  { slug: "vs-chatgpt", title: "Qwen AI vs ChatGPT", image: "/wp-content/uploads/2025/06/Qwen-AI-vs-ChatGPT.webp" },
  { slug: "quark", title: "Alibaba's Qwen-Powered Quark", image: "/wp-content/uploads/2025/03/Alibabas-Qwen-Powered-Quark-The-Flagship-AI-Assistant-Redefining-User-Experience.webp" },
  { slug: "takes-on-deepseek", title: "Alibaba's Qwen AI takes on DeepSeek", image: "/wp-content/uploads/2025/03/qwen-taps-Nvidia-RTX-4090.webp" },
  { slug: "vs-gemma-3-27b", title: "QwQ 32B vs Gemma 3 27B", image: "/wp-content/uploads/2025/03/qwen-32b-vs-gemma-3-27b.webp" },
  { slug: "manus-alliance", title: "Manus AI Partners with Qwen", image: "/wp-content/uploads/2025/03/Manus-AI-Partners-with-Qwen.webp" },
  { slug: "manus", title: "Manus AI & Qwen", image: "/wp-content/uploads/2025/03/Manus-AI-Qwen.webp" },
  { slug: "vs-deepseek-r1", title: "QwQ-32B vs DeepSeek R1", image: "/wp-content/uploads/2025/03/QwQ-32B-versus-Deepseek-R1.webp" },
];
---
<BaseLayout title="Qwen Blog" seoTitle="Qwen Blog: Latest Qwen AI News, Benchmarks & Guides" seoDescription="">
  <article class="qwen-container">
    <h1>Qwen Blog</h1>
    <h2>Featured</h2>
    <div class="blog-posts-list">
      {posts.map(post => (
        <a href={`/${post.slug}/`} class="qwen-post qwen-card">
          <div class="qwen-post-media">
            <img src={post.image} alt={post.title} class="qwen-post-image" loading="lazy" />
          </div>
          <div class="qwen-post-content">
            <h3 class="qwen-post-title">{post.title}</h3>
          </div>
        </a>
      ))}
    </div>
  </article>
</BaseLayout>
